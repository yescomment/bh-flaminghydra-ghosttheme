{{!< default}}

<div class="polyptich aside-on-right homepage">

<h1 class="fancy">On Fire <small>and</small> Hard to Kill</h1>

<main class="site-main">

  <div class="post-feed gh-feed gh-canvas">

    <h2 class="fancy"><small>From the</small> Current Issue</h2>
    {{> recent-nonissues}}

    <h2 class="fancy"><small>All</small> Issues</h2>
    {{> issue-index}}
  </div>

</main>

<aside class="fh-box">
  <h2 class="fancy"><small>Our</small> Contributors</h2>
  {{> author-index}}
</aside>

</div>
